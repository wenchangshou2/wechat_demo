.content {
  height: 50px;
  line-height: 30px;
}
.header{
  padding-top: 30px;
  width:90%;
  margin: 0 auto;
  border-bottom: 1px dashed #000;
  padding-bottom: 20px;
  height:60px;
  
}
.header-left{
  width:30%;
  height:60px;
  float: left;
}
.header-right{
  position: absolute;
  right: 20px;
  width:50px;
  height:60px;
}
.logo{
  width:50px;
  height:50px;
}
.main-title{
  display: block;
  margin-bottom: 10px;
}
.sub-title{
  display :block;
  font-size:12px;
  color:silver;
}

.title {
  width: 100%;
  background: red;
  height: 20px;
  font-size: 10px;
}
.contactInformation{
  height:100px;
}

.contact{
  height:30px;
  margin-top: 10px;
  margin-left: 10px;
}
.contact-icon{
  width:20px;
  height:20px;
  margin-left: 10px;
  color: #d9d9d9;
}
.contact-content{
  position: absolute;
  font-size: 12px;
  margin: 0 auto;
  margin-left: 20px;
}
.contact-right{
  position: absolute;
  right:20px;
  font-size: 12px;
  width:10px;
  height:10px;
  color:#d9d9d9;
  margin: 0 auto;
}
.clearfix::after{
  content: '';
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}
.imageList{
  display: absolute;
  margin-left: 10px;
}
.imageList image{
  width:30%;
  height:120px;
  float: left;
  margin:2px;
}
.imageList::after{
  clear: both;
}
.relative{
    clear: left;
  position: relative;
}
.bottom{
  position: fixed;
  width:100%;
  text-align: center;
  margin:0 auto;
  bottom:20px;
  font-size: 12px;
  color: green;
}
